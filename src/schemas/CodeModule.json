{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "#CodeModule",
  "description": "A software module. Such as an npm_module or java_library.",
  "type": "object",
  "allOf": [
    {
      "$j1ref": "#Entity",
      "$ref": "Entity.json"
    },
    {
      "properties": {
        "public": {
          "description": "Indicates if this is a public module.",
          "type": "boolean"
        }
      },
      "required": []
    }
  ]
}
